{
  "name": "userts",
  "version": "1.0.0",
  "description": "A simple typescript project",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "build": "tsc",
    "build:run": "yarn build && yarn start",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "sucrase": "^3.34.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExodiumTeam/userts.git"
  },
  "keywords": [
    "UserDev",
    "Typescript",
    "Estructure"
  ],
  "bugs": {
    "url": "https://github.com/ExodiumTeam/userts/issues"
  },
  "homepage": "https://github.com/ExodiumTeam/userts#readme",
  "author": "UserDev",
  "license": "MIT"
}
